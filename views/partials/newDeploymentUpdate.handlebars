{{#each deployments}}
<li class="deployment {{this._status}} {{this.deployment_id}}" id="{{this._modifiedTime}}" onclick="$('#{{this.deployment_id}}').toggle('slow');">
  <div class="environment"><span class="title">Environment:</span> {{this.objective.target}}</div>
  <div class="lastUpdate"><span class="title">Last Updated:</span> {{this._modifiedTime}}</div>
  <div class="status"><span class="title">Status:</span> {{this._status}}</div>
  <ol class="components">
    <li class="component header">
      <div class="component paramName">Parameter(s)</div>
      <div class="component paramDesc">Description</div>
      <div class="component paramArtifact">Artifact ID</div>
    </li>
    {{#each this.objective.components}}
    <li class="component">
      <div class="component paramName">{{this}}</div>
      <div class="component paramDesc">WIP</div>
      <div class="component paramArtifact">WIP</div>
    </li>
    {{/each}}
  </ol>
  <div class="moreInfo" id="{{this.deployment_id}}">
    <div class="deployment_id"><span class="title">Deployment ID:</span> {{this.deployment_id}}</div>
    <div class="stackInfo"><span class="title">Stack #:</span>WIP</div>
    <div class="startTime"><span class="title">Start time:</span> {{this._createdTime}}</div>
    <div class="endTime"><span class="title">End time:</span> {{this._modifiedTime}}</div>
  </div>
</li>
{{/each}}
