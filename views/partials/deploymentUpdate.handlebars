{{#reverse deployments}}
  <tr class="deployments {{this.deployment_id}}" id="{{this._modifiedTime}}">
    <td class="{{this._status}}">{{this._status}}</td>
    <td>{{this.deployment_id}}</td>
    <td>{{this.objective.target}}</td>
    <td>{{#each this.objective.components}}{{this}}</br>{{/each}}</td>
    <td>{{this.objective.time}}</td>
  </tr>
{{/reverse}}
